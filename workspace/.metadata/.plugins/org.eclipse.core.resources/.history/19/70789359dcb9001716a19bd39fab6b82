package br.senai.sp.informatica.mobileb.calculadora;

public enum Operacao {
	SOMA {
		public double calcule(double num1, double num2) {
			return num1 + num2;
		}
	},
	SUBTRACAO {
		public double calcule(double num1, double num2) {
			return num1 - num2;
		}
	};

	public abstract double calcule(double num1, double num2);
}